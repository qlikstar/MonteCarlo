{
  "configuration": {
    "status": "error",
    "name": "MonteCarloSimulation",
    "packages": "com.decipherx.fintech.montecarlo",
    "ThresholdFilter": {
      "level": "info"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d [%t] %-5p %c - %m%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}